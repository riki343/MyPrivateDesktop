{% extends '::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets
        '@css'
    %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>

    {% javascripts
        '@vendors'
        '@modules'
        '@models'
        '@services'
        '@controllers'
        '@components'
        output="/js/all.js"
    %}
        <script type="text/javascript" rel="script" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript" rel="script">
        new WOW().init();
//        var section = document.getElementById('my-private-desktop-app');
//        angular.bootstrap(section, [ 'kernel' ]);
    </script>
{% endblock %}